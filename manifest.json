{
  "name": "Konachan Prettify",
  "version": "1.0",
  "description": "Adjust Konachan website UI for better experience",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://konachan.net/post?*",
        "http://konachan.net/post/",
        "http://konachan.net/post",
        "http://konachan.com/post?*",
        "http://konachan.com/post/",
        "http://konachan.com/post"
      ],
      "css": [],
      "js": [
        "konachanPosts.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://yande.re/post/",
        "https://yande.re/post",
        "https://yande.re/post?*"
      ],
      "css": [],
      "js": [
        "yandePosts.js"
      ],
      "run_at": "document_end"
    }
  ]
}
