{
  "name": "Konachan Posts",
  "version": "1.0",
  "description": "Enlarge images of konachan preview post list for a clear view",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://yande.re/post/",
        "https://yande.re/post",
        "https://yande.re/post?*",
        "http://konachan.net/post?*",
        "http://konachan.net/post/",
        "http://konachan.net/post",
        "http://konachan.com/post?*",
        "http://konachan.com/post/",
        "http://konachan.com/post"
      ],
      "css": [],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ]
}